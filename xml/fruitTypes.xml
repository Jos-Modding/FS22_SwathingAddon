    <fruitTypeConverters>
        <fruitTypeConverter name="FORAGEHARVESTER">
			<converter from="WHEAT2"    to="WHEAT"  	   factor="0.29" windrowFactor="0.85" />
			<converter from="BARLEY2"   to="BARLEY"  	   factor="0.31" windrowFactor="0.85" />
			<converter from="OAT2"    	to="OAT"  		   factor="0.19" windrowFactor="0.85" />
			<converter from="CANOLA2"   to="CANOLA"  	   factor="0.19" windrowFactor="0.85" />
        </fruitTypeConverter>
		<fruitTypeConverter name="MOWER">
            <converter from="WHEAT"     to="WHEAT_WINDROW" factor="1.0" windrowFactor="1.0" />
            <converter from="BARLEY"    to="BARLEY_WINDROW" factor="1.0" windrowFactor="1.0" />
            <converter from="OAT"       to="OAT_WINDROW"   factor="1.0" windrowFactor="1.0" />
			<converter from="CANOLA"    to="CANOLA_WINDROW"  factor="1.0" windrowFactor="1.0" />
        </fruitTypeConverter>
        <fruitTypeConverter name="SWATHER">
            <converter from="WHEAT"     to="WHEAT_WINDROW" factor="1.0" windrowFactor="1.0" />
            <converter from="BARLEY"    to="BARLEY_WINDROW" factor="1.0" windrowFactor="1.0" />
            <converter from="OAT"       to="OAT_WINDROW"   factor="1.0" windrowFactor="1.0" />
			<converter from="CANOLA"    to="CANOLA_WINDROW"  factor="1.0" windrowFactor="1.0" />
        </fruitTypeConverter>
        <!-- MaizePlus -->
        <fruitTypeConverter name="MOWERCONDITIONER">
            <converter from="GRASS"		to="GRASS_WINDROW"		factor="1.0"	windrowFactor="1.0"/>
            <converter from="MEADOW"	to="GRASS_WINDROW"		factor="1.0"	windrowFactor="1.0"/>
            <converter from="WHEAT"		to="CROP_WINDROW"		factor="1.0"	windrowFactor="1.0"/>
            <converter from="BARLEY"	to="CROP_WINDROW"		factor="1.0"	windrowFactor="1.0"/>
            <converter from="OAT"		to="CROP_WINDROW"		factor="1.0"	windrowFactor="1.0"/>
        </fruitTypeConverter>
    </fruitTypeConverters>
</map>
